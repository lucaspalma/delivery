input FormPdv {
	tradingName: String!
	ownerName: String!
	document: String!
	address: AddressForm!
	coverageArea: CoverageAreaForm!
}

type Pdv {
 	id: ID!
 	tradingName: String!
 	ownerName: String!
 	document: String!
 	address: Address!
 	coverageArea : CoverageArea!
}

type Query {
	findAllPdvs: [Pdv]!
	findPdv(id: ID!): Pdv!
	searchPdv(lng: Float!, lat: Float!): [Pdv]!
	
}

type Mutation {
  newPdv( form: FormPdv! ) : Pdv!
}
