type Pdv {
 	id: ID!
 	tradingName: String!
 	ownerName: String!
 	document: String!
 	address: Address!
 	coverageArea : CoverageArea!
}

type Query {
	findAllPdvs: [Pdv]!
	findPdv(id: ID!): Pdv!
	searchPdv(lng: Float!, lat: Float!): Pdv!
	
}

type Mutation {
  newPdv( tradingName: String!, ownerName: String!, document: String!, address: AddressInput!, coverageArea: CoverageAreaInput!) : Pdv!
}
